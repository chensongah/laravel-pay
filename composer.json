{
    "name": "smartjson/laravel-pay",
    "description": "专注 Alipay 和 WeChat 的 laravel 支付扩展包",
    "keywords": ["alipay", "wechat", "pay", "laravel", "package"],
    "support": {
        "issues": "https://github.com/smartjsoncom/laravel-pay/issues",
        "source": "https://github.com/smartjsoncom/laravel-pay"
    },
    "authors": [
        {
            "name": "SmartJson",
            "email": "SmartJson@smartjson.cn"
        }
    ],
    "require": {
        "illuminate/support": "^5.1",
        "smartjson/pay": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "SmartJson\\LaravelPay\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartJson\\LaravelPay\\PayServiceProvider"
            ],
            "aliases": {
                "Pay": "SmartJson\\LaravelPay\\Facades\\Pay"
            }
        }
    },
    "license": "MIT"
}
